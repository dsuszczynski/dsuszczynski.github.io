---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>{{ site.url }}/</loc>
    <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
  </url>
  {% for page in site.pages %}{% if page.url != '/404.html' and page.url != '/sitemap.xml' and page.url != '/robots.txt' %}
  <url>
    <loc>{{ site.url }}{{ page.url }}</loc>
    <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.8</priority>
  </url>
  {% endif %}{% endfor %}
  {% for article in site.articles %}
  <url>
    <loc>{{ site.url }}{{ article.url }}</loc>
    <lastmod>{{ article.date | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.7</priority>
  </url>
  {% endfor %}
  {% for case_study in site.case_studies %}
  <url>
    <loc>{{ site.url }}{{ case_study.url }}</loc>
    <lastmod>{{ case_study.date | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.7</priority>
  </url>
  {% endfor %}
</urlset>
